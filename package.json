{
  "name": "@expo/nullthrows",
  "version": "2.0.0",
  "description": "An idiomatic way to enforce values not to be null nor undefined, with first-class support for TypeScript and ES modules",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/nullthrows.ts",
      "import": "./build/nullthrows.js",
      "require": "./build/nullthrows.js"
    }
  },
  "files": [
    "build",
    "src",
    "!src/**/test"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "prepare": "npm run clean && npm run build",
    "test": "node --disable-warning=ExperimentalWarning --experimental-strip-types --test src/test/*-test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/nullthrows.git"
  },
  "keywords": [
    "nonnull",
    "nullability",
    "typescript"
  ],
  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/nullthrows/issues"
  },
  "homepage": "https://github.com/expo/nullthrows#readme",
  "volta": {
    "node": "24.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "typescript": "^5.9.2"
  }
}
